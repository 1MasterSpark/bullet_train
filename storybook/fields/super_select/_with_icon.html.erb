<% content_for :story do %>
  <% @tangible_thing = Scaffolding::CompletelyConcrete::TangibleThing.new(super_select_value: story_params[:super_select_value]) %>

  <%= form_with model: [@tangible_thing], url: "#", local: true do |form| %>

    <%= render 'shared/fields/super_select', form: form, method: :super_select_value,
      other_options: { accepts_new: true, left_icon: true, icon: 'far fa-sort-amount-up' }, choices: ['Option 1', 'Option 2', 'Option3'].map { |option, i| [option, i] } %>
  <% end %>
<% end %>

<% content_for :source do %>
  <%= storybook_auto_source %>
<% end %>
